{
  "name": "az-func-as-a-graph",
  "displayName": "az-func-as-a-graph",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "browser": "./dist/web/extension.js",

  "contributes": {

    "commands": [
      {
        "command": "az-func-as-a-graph.ShowGraph",
        "title": "az-func-as-a-graph: Show..."
      }
    ],

    "menus": {
      "explorer/context": [
        {
          "command": "az-func-as-a-graph.ShowGraph",
          "when": "resourceFilename == host.json",
          "group": "az-func-as-a-graph@1"
        }
      ]
    }

  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/vscode": "^1.75.1",
    "@types/mocha": "^10.0.1",
    "eslint": "^8.34.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-web": "^0.0.35",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "@types/webpack-env": "^1.18.0",
    "assert": "^2.0.0",
    "process": "^0.11.10",
    "func-project-parser": "file:../func-project-parser"
  }
}